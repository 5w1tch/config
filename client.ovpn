# https://github.com/drduh/config/blob/master/client.ovpn
client
remote 1.2.3.4 443
proto udp
tls-version-min 1.2
tls-cipher TLS-ECDHE-RSA-WITH-AES-256-GCM-SHA384
ncp-ciphers AES-256-GCM
cipher AES-256-GCM
auth SHA512
tls-auth ta.key 1
remote-cert-tls server
remote-cert-eku "TLS Web Server Authentication"
dev tun
tun-ipv6
redirect-gateway
dhcp-option DNS 10.8.0.1
comp-lzo
persist-tun
persist-key
ping 60
ping-restart 120
ping-timer-rem
verb 3
